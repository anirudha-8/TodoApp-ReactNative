{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@state/*": [
        "./src/state/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@__tests__/*": [
        "__tests__/*"
      ],
    }
  }
}